alias bec='bundle exec cap'
alias inknotes='cat ~/dotfiles/ink_notes.txt'

# git
alias amend="g amend"
alias p="g ap"
alias c="g commit -m"
alias slog="g slog"
alias branches="g branches"
alias unstage="g unstage"
